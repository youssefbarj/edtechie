<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <rect width="32" height="32" fill="#1e293b"/>
  <!-- Owl favicon -->
  <g transform="translate(16, 16)">
    <!-- Branch -->
    <line x1="-6" y1="-2" x2="-2" y2="-6" stroke="#f59e0b" stroke-width="1" stroke-linecap="round"/>
    <!-- Leaves -->
    <ellipse cx="-5" cy="-4" rx="1" ry="2" fill="#f59e0b" transform="rotate(-30 -5 -4)"/>
    <ellipse cx="-2" cy="-7" rx="0.8" ry="1.5" fill="#f59e0b" transform="rotate(-45 -2 -7)"/>
    
    <!-- Owl body -->
    <ellipse cx="0" cy="4" rx="6" ry="8" fill="#f59e0b"/>
    
    <!-- Owl head -->
    <circle cx="0" cy="0" r="6" fill="#f59e0b"/>
    
    <!-- Eyes -->
    <circle cx="-2.5" cy="-1" r="1.5" fill="#1e293b"/>
    <circle cx="2.5" cy="-1" r="1.5" fill="#1e293b"/>
    <circle cx="-2.5" cy="-1" r="0.8" fill="#f59e0b"/>
    <circle cx="2.5" cy="-1" r="0.8" fill="#f59e0b"/>
    
    <!-- Beak -->
    <polygon points="0,1 -1,2.5 1,2.5" fill="#1e293b"/>
    
    <!-- Wing detail -->
    <path d="M -4 2 Q -6 6 -4 10" stroke="#1e293b" stroke-width="0.8" fill="none"/>
    
    <!-- Feet -->
    <ellipse cx="-2" cy="12" rx="1" ry="0.8" fill="#f59e0b"/>
    <ellipse cx="2" cy="12" rx="1" ry="0.8" fill="#f59e0b"/>
  </g>
</svg>
